{
  "name": "meiuca-challenge",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "token": "node src/tokens/tokens.js && json-to-scss src/tokens/variables.json",
    "build": "node src/tokens/tokens.js && json-to-scss src/tokens/variables.json && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "storybook": "ng run meiuca-challenge:storybook",
    "build-storybook": "node src/tokens/tokens.js && json-to-scss src/tokens/variables.json && ng run meiuca-challenge:build-storybook",
    "json-to-scss": "json-to-scss"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular/cli": "~15.2.4",
    "@angular/compiler-cli": "^15.2.0",
    "@storybook/addon-essentials": "^7.0.18",
    "@storybook/addon-interactions": "^7.0.18",
    "@storybook/addon-links": "^7.0.18",
    "@storybook/angular": "^7.0.18",
    "@storybook/blocks": "^7.0.18",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.5.0",
    "json-to-scss": "^1.6.2",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.18",
    "typescript": "~4.9.4"
  }
}
